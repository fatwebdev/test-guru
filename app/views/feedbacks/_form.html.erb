<%= form_with model: @feedback, local: true do |f| %>
  <%= render 'shared/errors', entity: @feedback %>

  <p class="form-group">
    <%= f.label :title %>
    <br/>
    <%= f.text_field :title, class: "form-control" %>
  </p>

  <p class="form-group">
    <%= f.label :body %>
    <br/>
    <%= f.text_area :body, class: "form-control" %>
  </p>


  <%= f.submit class: "btn btn-primary" %>
<% end %>